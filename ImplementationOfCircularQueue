#include <iostream>
using namespace std;
class Cqueue{
  public:
  int size;
  int *arr;
  int front ;
  int rear;

  Cqueue(int size){
    arr = new int[size];
    this->size = size;
    front= -1;
    rear = -1;
  }

  void push(int data){
    if(front == -1 && rear == -1){
      front++;
      rear++;
      arr[front] = data;
      arr[rear] = data;
    }
    else if(rear == size -1 && front == 0 || rear == front-1){
      cout<<"OVERFLOW"<<endl;
    }
    else if(rear == size-1 && front!=0){
      rear = 0;
      arr[rear] = data;
      rear++;
    }
    else{
      rear++;
      arr[rear] = data;
      
    }
  }
  void pop(){
    if(front == rear){
      arr[front] = -1;
      arr[rear] = -1;
      front = -1;
      rear = -1;

    }
    else if(front == -1 && rear == -1){
      cout<<"UNDERFLOW"<<endl;
    }
    else if(front == size-1){
     arr[front] = -1;
     front = 0;
      
    }
    else{
      arr[front] = -1;
      front++;
    }
  }
     void print(){
   cout<<"Queue is :";
   for(int i=0;i<size;i++){
    cout<<arr[i]<<" ";
   }  cout<<endl;
  }

};
int main() {
  Cqueue q(5);
  q.push(3);
  q.print();

   q.push(3);
  q.print();

   q.push(3);
  q.print();

   q.push(3);
  q.print();

   q.push(3);
  q.print();

  q.pop();
    q.print();

  q.push(100);
  q.print();

}
